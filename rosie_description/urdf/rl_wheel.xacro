<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="tracer_wheel">
    <xacro:include filename="$(find rosie_description)/urdf/rl_castor.xacro" />
    <xacro:macro name="rl_wheel" params="wheel_prefix *joint_pose">
        <link name="${wheel_prefix}_wheel_link">
            <inertial>
                <mass value="1" />
                <origin xyz="0 0 0" />
                <inertia ixx="0.3131" ixy="0" ixz="0" iyy="0.3131" iyz="0" izz="0.1361" />
                <!--<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />-->
            </inertial>

        </link>

        <joint name="${wheel_prefix}_wheel" type="continuous">
            <!--<origin
                  xyz="0.14547 0.20835 -0.07424"
                  rpy="0 1.57 0" />-->
            <parent link="rl_castor_link"/>
            <child link="${wheel_prefix}_wheel_link"/>
            <dynamics damping="0.0" friction="0.0"/>
            <xacro:insert_block name="joint_pose"/>
            <axis xyz="0 0 1" rpy="0 0 0" />
        </joint>
    </xacro:macro>

</robot>
